    "axios": "^1.6.5",
    "drizzle-orm": "^0.29.3",
    "fastify": "^4.25.2",
    "fastify-guard": "^2.0.0",
    "fastify-zod": "^1.4.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "zennv": "^0.1.1",
    "zod": "^3.22.4"
  }
