  },
  "engines": {
    "node": ">=18.17.0",
    "npm": "please-use-yarn"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "18.4.3",
    "@types/jest": "29.5.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "20.4.5",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "commitlint": "18.4.3",
    "drizzle-kit": "^0.20.10",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.6.2",
    "lefthook": "1.5.5",
    "prettier": "3.0.0",
    "rimraf": "5.0.1",
    "ts-jest": "29.1.1",
